{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "max-lines-per-function": [
      "warn",
      {
        "max": 50,
        "skipBlankLines": true,
        "skipComments": true
      }
    ],
    "complexity": ["error", 10],
    "indent": ["warn", 4],
    "max-len": ["warn", 120]
  }
}
